  "extends": [
    "config:recommended"
  ],
  "assignees": [
    "geanttechnology"
  ],
  "ignorePaths": [
    "assets/"
  ],
  "ansible": {
    "managerFilePatterns": ["Ansible/.+\\.ya?ml$"]
  },
  "terraform": {
    "managerFilePatterns": ["Terraform/.+\\.tf$"]
  },
  "customManagers": [
    {
      "customType": "regex",
      "description": "Update age binary package",
      "managerFilePatterns": ["Ansible/roles/app-age/vars/main.yml"],
      "matchStrings": [
        "#\\s*renovate:\\s*datasource=(?<datasource>[^\\s]+)\\s*depName=(?<depName>[^\\s]+)(?:\\s+versioning=(?<versioning>[^\\s]+))?[^\\n]*?\\s*age_ver:\\s*['\"](?<currentValue>[^'\"]+)['\"]"
      ],
      "versioningTemplate": "{{#if versioning}}{{{versioning}}}{{else}}semver{{/if}}"
    },
    {
      "customType": "regex",
      "description": "Update age binary package sha256 checksum",
      "managerFilePatterns": ["Ansible/roles/app-age/vars/main.yml"],
      "matchStrings": [
        "#\\s*renovate:\\s*datasource=(?<datasource>[^\\s]+)\\s*depName=(?<depName>[^\\s]+)\\s*packageName=(?<packageName>[^\\s]+)(?:\\s+versioning=(?<versioning>[^\\s]+))?[^\\n]*?\\s*age_checksum:\\s*sha256:(?<currentValue>[a-f0-9]{64})"
      ],
      "versioningTemplate": "{{#if versioning}}{{{versioning}}}{{else}}semver{{/if}}",
      "autoReplaceStringTemplate": "sha256:{{newDigest}}"
    },
    {
